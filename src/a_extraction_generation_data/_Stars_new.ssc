// Funkcja losowa w zakresie
function randomBetween(min, max) {
    return Math.random() * (max - min) + min;
}

// Funkcja ustawiająca losową lokalizację na Ziemi
function setRandomLocation() {
	const latitude = randomBetween(-60, 60); // Zakres szerokości geograficznej
	const longitude = randomBetween(-180, 180); // Zakres długości geograficznej
	core.setObserverLocation(longitude, latitude, 100); // Wysokość 100 m
	core.debug("-  Szerokość: " + latitude + "°");
	core.debug("-  Długość: " + longitude + "°");
}

// Funkcja ustawiająca losowy fragment nieba
function setRandomPosition() {
	const altitude  = randomBetween(40, 90);
	const azimuth  = randomBetween(-180, 180); 
	core.moveToAltAzi(altitude , azimuth, 0.1 );
	core.debug("-  Wysokość: " + altitude + "°");
	core.debug("-  Azymut: " + azimuth + "°");
}

function parseHhMmString(s) {
    if (!s || typeof s !== "string") return null;
    var m = s.trim().match(/^(\d{1,2})\s*h(?:\s*(\d{1,2})\s*m)?(?:\s*(\d{1,2})\s*s)?$/i);
    if (!m) return null;
    var hh = parseInt(m[1], 10);
    var mm = m[2] !== undefined ? parseInt(m[2], 10) : 0;
    var ss = m[3] !== undefined ? parseInt(m[3], 10) : 0;
    if (mm < 0 || mm > 59 || ss < 0 || ss > 59) return null;
    return { h: hh, m: mm, s: ss };
}

function timeStringToDoubleHours(s) {
    var t = parseHhMmString(s);
    if (!t) return NaN;
    return t.h + t.m/60 + t.s/3600;
}

// funkcja ustawiająca losowy czas w nocy
function setRandomTime() {
    // Losowa data w latach 2000–2025
    var year = 2000 + Math.floor(Math.random() * 26);  // 2000–2025
    var month = 1 + Math.floor(Math.random() * 12);    // 1–12
    var day = 1 + Math.floor(Math.random() * 28);      // 1–28 (bezpiecznie dla wszystkich miesięcy)

    // Ustaw datę na południe, aby mieć pewność, że Słońce jest "obecne" tego dnia
	var dateString= year + "-" + ("0" + month).slice(-2)  + "-" + ("0" + day).slice(-2) + "T00:00:00";
	core.setDate(dateString, "local");
    core.wait(1); // Poczekaj chwilę, aby Stellarium przeliczyło pozycje obiektów

    // Oblicz zachód i wschód Słońca
    var sunInfo = core.getObjectInfo("Sun");
    var sunset = timeStringToDoubleHours(sunInfo.set);
    var sunrise = timeStringToDoubleHours(sunInfo.rise) ; 
    var dateStr = core.getDate(spec = "local");
    core.debug("Data: " + dateStr);
    core.debug("Zachód słońca: " + sunInfo.set + " - " + sunset);
    core.debug("Wschód słońca: " + sunInfo.rise + " - " + sunrise);

    if ((sunset !== sunset)|| (sunrise !== sunrise)) {
        core.debug("Brak danych o wschodzie/zachodzie Słońca dla tej daty.");
        return false;
    }

   // Dodaj 24 godziny, aby mieć ciągłość czasu nocnego
    if (sunset > sunrise) {
    	sunrise = sunrise + 24; 
    }

    if (sunset - sunrise > 2) {
        core.debug("Dzień polarny - brak nocy dla tej daty i lokalizacji.");
        return false;
    }

    // Odległość jednej godziny w dniach juliańskich
   // var oneHour = 1.0 / 24.0;

    // Zakres między zachodem +1h a wschodem -1h
    var start = (sunset + 1.0) / 24;
    var end = (sunrise - 1.0) / 24;


    // Aktualny czas juliański po ustawieniu daty
    var jd = core.getJDay();

    // Losowy moment w tym zakresie
    var randomJD = jd + start + Math.random() * (end - start);
    core.debug("Losowy czas juliański: " + jd + " -> " + randomJD);

    // Ustawienie w Stellarium
    core.setJDay(randomJD);
    core.wait(1); // Poczekaj chwilę, aby Stellarium przeliczyło pozycje obiektów

    // Log
    var dateStr = core.getDate(spec = "local");
    core.debug("Losowo ustawiono datę i czas: " + dateStr + " - " + randomJD);
    return true;
}



// funkcja tworząca plik CSV
function createCSV(fileName, folder)
{
	core.resetOutput();
	core.output('Star_ID,Name,X,Y')

	var width = core.getScreenWidth();
	var height = core.getScreenHeight();
	for(var st in starsDict)
	{
		var map=core.getObjectInfo(st);
		var pos = core.getScreenXYFromAltAzi (map["altitude"], map["azimuth"]);
		if ((pos["x"] >= 0) &&  (pos["x"] < width) && (pos["y"] >= 0) &&  (pos["y"] < height))
		{
			core.output(starsDict[st] +  ',' +  st + ',' + pos["x"] + ',' +  pos["y"]);
		}
	}
	core.saveOutputAs(folder + '/' + fileName + '.csv');
}

function captureScreenshot(fileName, folder) {
	core.debug(fileName);
	core.screenshot(fileName, false, folder, true, "png"); // Zapis obrazu w rozdzielczości Full HD
	core.wait(1); // Krótka pauza na zapisanie obrazu
}

// Funkcja zapisująca oryginalne flagi i ustawiająca tryb testowy
function saveOriginalFlagsAndApplyTest() {
    const orig = {
        landscape: LandscapeMgr.getFlagLandscape(),
        fog: LandscapeMgr.getFlagFog(),
        illumination: LandscapeMgr.getFlagIllumination(),
        labels: LandscapeMgr.getFlagLabels(),
        polyLineDisplayed: LandscapeMgr.getFlagPolyLineOnlyDisplayed(),
        landscapeSetsLocation: LandscapeMgr.getFlagLandscapeSetsLocation(),
        atmosphere: LandscapeMgr.getFlagAtmosphere(),
        meridianLine: GridLinesMgr.getFlagMeridianLine()
    };

    // ustawienia do testów
    core.setGuiVisible(false);
    LandscapeMgr.setFlagLandscape(false);
    LandscapeMgr.setFlagFog(false);
    LandscapeMgr.setFlagIllumination(false);
    LandscapeMgr.setFlagLabels(false);
    LandscapeMgr.setFlagPolyLineOnlyDisplayed(false);
    LandscapeMgr.setFlagLandscapeSetsLocation(false);
    LandscapeMgr.setFlagAtmosphere(true);
    GridLinesMgr.setFlagMeridianLine(false);

    return orig;
}

// Funkcja przywracająca zapisane ustawienia
function restoreOriginalFlags(orig) {
    core.setGuiVisible(true);
    try {
        LandscapeMgr.setFlagLandscape(!!orig.landscape);
        LandscapeMgr.setFlagFog(!!orig.fog);
        LandscapeMgr.setFlagIllumination(!!orig.illumination);
        LandscapeMgr.setFlagLabels(!!orig.labels);
        LandscapeMgr.setFlagPolyLineOnlyDisplayed(!!orig.polyLineDisplayed);
        LandscapeMgr.setFlagLandscapeSetsLocation(!!orig.landscapeSetsLocation);
        LandscapeMgr.setFlagAtmosphere(!!orig.atmosphere);
        GridLinesMgr.setFlagMeridianLine(!!orig.meridianLine);
    } catch (e) {
        core.debug("Błąd przy przywracaniu ustawień: " + e);
    }
}

