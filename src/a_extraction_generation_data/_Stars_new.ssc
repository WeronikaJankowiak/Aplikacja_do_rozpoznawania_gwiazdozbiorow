// Główna funkcja skryptu
function main() {
    let imagesNumber = 0;
    let maxImagesNumber = 10;

    // zapisz i zastosuj ustawienia testowe (zwrócony obiekt użyj do przywrócenia)
    const _savedFlags = saveOriginalFlagsAndApplyTest();

    try {
        core.debug("Start...");

        for( var  i=50000; i<55000; i++)
        {
            setRandomLocation();
            while (!setRandomTime()) {} // Powtarzaj aż zwróci true
            setRandomPosition(); //Ustawienie losowego fragmentu nieba

	  get_Info() 
	

            const folder = 'C:/_Gwiazdozbiory/Aplikacja/src/a_extraction_generation_data/Screenshots_5';
            const fileName = 'sky_' + String(i).padStart(6, '0');
                
            captureScreenshot(fileName, folder);
            createCSV(fileName, folder);
	  core.debug("------------------------------------------.");
        }
    } finally {
        // przywróć oryginalne ustawienia na końcu (zawsze)
        restoreOriginalFlags(_savedFlags);
    }
}

